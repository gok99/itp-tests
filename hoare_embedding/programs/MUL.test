MUL = Stmt.SEQ(
    Stmt.ASSIGN("r", lambda s: 0),
    Stmt.WHILE_DO(
        lambda s: s["n"] != 0,
        Stmt.SEQ(
            Stmt.ASSIGN("r", lambda s: s["r"] + s["m"]),
            Stmt.ASSIGN("n", lambda s: s["n"] - 1)
        )
    )
)

